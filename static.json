{
    "root": "public/",
    "headers": {
        "*/**": {
            "Cache-Control": "public, max-age=0, must-revalidate"
        },
        "*/**.js": {
            "Cache-Control": "public, max-age=31536000, inmutable"
        },
        "*/static/**": {
            "Cache-Control": "public, max-age=31536000, inmutable"
        },
        "/icons/*.png": {
            "Cache-Control": "public, max-age=31536000, inmutable"
        }
    },
    "https_only": true,
    "error_page": "404.html"
}